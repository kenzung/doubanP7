{
  "compilerOptions": {
    "outDir": "build/dist",
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "rootDir": "src",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
  },
  "exclude": [
    "node_modules",
    "build",
    "scripts",
    "config",
    "public",
    ".storybook",
    "src/registerServiceWorker.js"
  ]
}